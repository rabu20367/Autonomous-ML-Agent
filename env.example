# Environment variables for Autonomous ML Agent

# LLM Configuration
LLM_PROVIDER=openai
LLM_MODEL=gpt-4
LLM_API_KEY=your_openai_api_key_here
LLM_MAX_TOKENS=4000
LLM_TEMPERATURE=0.1

# Alternative LLM Configuration (uncomment to use)
# LLM_PROVIDER=anthropic
# LLM_MODEL=claude-3-sonnet-20240229
# LLM_API_KEY=your_anthropic_api_key_here

# Optimization Settings
OPT_N_TRIALS=50
OPT_TIMEOUT=3600
OPT_CV_FOLDS=5
OPT_SCORING=accuracy

# Database Settings
EXPERIENCE_DB_PATH=experiences.db
CHROMA_DB_PATH=./chroma_db

# Logging
LOG_LEVEL=INFO
LOG_FILE=autonomous_ml.log
